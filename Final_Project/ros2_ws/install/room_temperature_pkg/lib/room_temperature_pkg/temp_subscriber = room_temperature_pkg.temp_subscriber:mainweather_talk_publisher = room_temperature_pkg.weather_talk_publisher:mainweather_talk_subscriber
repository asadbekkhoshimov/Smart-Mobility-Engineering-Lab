#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'room-temperature-pkg==0.0.0','console_scripts','temp_subscriber = room_temperature_pkg.temp_subscriber:mainweather_talk_publisher = room_temperature_pkg.weather_talk_publisher:mainweather_talk_subscriber'
__requires__ = 'room-temperature-pkg==0.0.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('room-temperature-pkg==0.0.0', 'console_scripts', 'temp_subscriber = room_temperature_pkg.temp_subscriber:mainweather_talk_publisher = room_temperature_pkg.weather_talk_publisher:mainweather_talk_subscriber')()
    )
